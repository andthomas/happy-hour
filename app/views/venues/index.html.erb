<div class="container" style="padding-top:160px">

    <% @venues.each do |v| %>
    <div class="col-xs-4">
      <div class="venueBox">
        <%= cl_image_tag v.image, :class => "venuePic" %>
        <div class="venueContents">
          <p><%= link_to v.name, venue_path(v.id) %> | <%= v.category %></p>
        </div>
      </div>
      <br><br>
        </div>
    <% end %>

</div>
