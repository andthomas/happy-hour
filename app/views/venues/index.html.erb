<div class="container">

    <% @venues.each do |v| %>
    <div class="col-xs-4">
      <div class="venueBox">
        <img class="venuePic" src="<%= v.image %>" alt=""></img>
        <div class="venueContents">
          <p><%= link_to v.name, venue_path(v.id) %> | <%= v.category %></p>
        </div>
      </div>
      <br><br>
        </div>
    <% end %>

</div>
