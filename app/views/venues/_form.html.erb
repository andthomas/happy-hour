<%= form_for(@venue, html: {multipart: true}) do |f| %>

  <p>
    <%= f.label :name %>
    <%= f.text_field :name, required: true %>

  <p>
    <%= f.label :location %>
    <%= f.text_field :location, required: true %>
  </p>

  <p>
    <%= f.label :category %>
    <%= f.text_field :category, required: true %>
  </p>

  <p>
    <%= f.label :description %>
    <%= f.text_field :description, required: true %>
  </p>

  <p>
    <%= f.label :image %>
    <%= f.cl_image_upload :image, required: true %>
  </p>

  <%= f.submit %>

<% end %>
